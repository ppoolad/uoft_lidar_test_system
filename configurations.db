#gpio
hpc1_chip_name=gpiochip3
num_hpc1=40
hpc0_chip_name=gpiochip2
num_hpc0=32
led_chip_name=gpiochip0
num_led=8
pb_chip_name=gpiochip1
num_pb=5

#general
runtime_ms=2000;
output_file=data_out4.txt
#rx
rx_dev_fifo=/dev/axis_fifo_0x00000000a0030000
